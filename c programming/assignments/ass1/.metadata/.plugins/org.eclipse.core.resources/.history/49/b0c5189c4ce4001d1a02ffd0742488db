#include <stdio.h>
void main ()
{
	char alpha;
	printf("enter an alphabet: ");
	fflush(stdin); fflush(stdout);
	scanf("%c",&alpha);
	if (alpha=='a'||alpha=='i'||alpha=='o'||alpha=='u'||alpha=='e') printf("%c is a vowel");
	else printf("%c is consonant");

}
